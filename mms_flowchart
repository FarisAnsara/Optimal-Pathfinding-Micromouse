digraph {
	A [label="Explorer Class (FloodFill)"]
	B [label="MoveMouse: Get Current Position"]
	C [label="Walls: Retrieve Wall Information"]
	D [label="Algorithm of Choice: Choose Next Position"]
	E [label="MoveMouse: Move Mouse to Next Position"]
	F [label="MMS API: Update Simulator"]
	G [label="Check Goal"]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
	F -> G
	G -> B [label="If not at goal" constraint=false]
	G -> End [label="If at goal" constraint=false]
}
